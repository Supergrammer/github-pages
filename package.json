		"@vitejs/plugin-vue": "^4.1.0",
		"@vue/eslint-config-prettier": "^7.1.0",
		"eslint": "^8.36.0",
		"eslint-plugin-vue": "^9.9.0",
		"gh-pages": "^5.0.0",
		"prettier": "^2.8.5",
